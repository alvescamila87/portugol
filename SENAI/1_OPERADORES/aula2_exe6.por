programa {
  funcao inicio() {
    inteiro num1, num2, quadrado
    escreva("Digite um número inteiro para N1: ")
    leia(num1)
    escreva("Digite um valor inteiro para N2: ")
    leia(num2)
    quadrado = (num1*num1) + (num2*num2)
    escreva("A soma do quadrdao desses dois números: ", num1, " e ", num2, " é: ", quadrado, ".")
    
  }
}
