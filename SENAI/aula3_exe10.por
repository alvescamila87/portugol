programa
{
	
	funcao inicio()
	{
		real n1, n2
		escreva("Informe o valor de N1: ")
		leia(n1)
		escreva("Informe o valor de N2: ")
		leia(n2)
		escreva("\nA multiplicação dos números: ", n1, " e ", n2, " é: ", n1*n2, ".")
		escreva("\nA divisão dos números: ", n1, " e ", n2, " é: ", n1/n2, ".")
	}
}

